{
  "name": "@config-bound/config-bound",
  "version": "0.0.3",
  "packageManager": "npm@10.5.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/RobertKeyser/ConfigBound"
  },
  "scripts": {
    "format": "prettier --write --config .config/.prettierrc --ignore-path .config/.prettierignore .",
    "format:ci": "prettier --check --config .config/.prettierrc --ignore-path .config/.prettierignore .",
    "test": "jest --config .config/jest.config.js",
    "test:dev": "TEST_USE_CONSOLE_LOGGER=true npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:verbose": "TEST_USE_CONSOLE_LOGGER=true npm run test -- --verbose",
    "lint": "npm run lint:code && npm run lint:markdown",
    "lint:ci": "npm run lint:code:ci && npm run lint:markdown:ci",
    "lint:code": "npm run lint:code:ci -- --fix",
    "lint:code:ci": "eslint --config .config/eslint.config.mjs .",
    "lint:markdown": "npm run lint:markdown:ci -- --fix",
    "lint:markdown:ci": "markdownlint-cli2 --config .config/.markdownlint-cli2.jsonc \"**/*.md\" \"#node_modules\"",
    "build": "tsc",
    "prepare": "husky"
  },
  "main": "./dist/configBound.js",
  "types": "./dist/configBound.d.ts",
  "exports": {
    ".": {
      "types": "./dist/configBound.d.ts",
      "require": "./dist/configBound.js",
      "import": "./dist/configBound.js"
    },
    "./section": {
      "types": "./dist/section/section.d.ts",
      "require": "./dist/section/section.js",
      "import": "./dist/section/section.js"
    },
    "./element": {
      "types": "./dist/element/element.d.ts",
      "require": "./dist/element/element.js",
      "import": "./dist/element/element.js"
    },
    "./bind": {
      "types": "./dist/bind/bind.d.ts",
      "require": "./dist/bind/bind.js",
      "import": "./dist/bind/bind.js"
    },
    "./bind/bindContext": {
      "types": "./dist/bind/bindContext.d.ts",
      "require": "./dist/bind/bindContext.js",
      "import": "./dist/bind/bindContext.js"
    },
    "./bind/binds/envVar": {
      "types": "./dist/bind/binds/envVar.d.ts",
      "require": "./dist/bind/binds/envVar.js",
      "import": "./dist/bind/binds/envVar.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "author": "Robert Keyser",
  "license": "MIT",
  "description": "A config registration library",
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-turbo": "^2.5.3",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jest-runner-groups": "^2.2.0",
    "markdownlint-cli2": "^0.18.0",
    "prettier": "3.5.3",
    "ts-jest": "^29.3.2",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "dependencies": {
    "joi": "^17.13.3"
  }
}
