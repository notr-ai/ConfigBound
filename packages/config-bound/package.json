{
  "name": "@config-bound/config-bound",
  "version": "0.0.5",
  "description": "A config registration library",
  "repository": {
    "type": "git",
    "url": "https://github.com/notr-ai/ConfigBound"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist coverage",
    "test": "jest",
    "lint": "eslint src/**/*.ts --fix",
    "lint:ci": "eslint src/**/*.ts",
    "format": "prettier --write --config ../../.config/.prettierrc --ignore-path ../../.config/.prettierignore src/**/*.ts",
    "format:ci": "prettier --check --config ../../.config/.prettierrc --ignore-path ../../.config/.prettierignore src/**/*.ts"
  },
  "main": "./dist/configBound.js",
  "types": "./dist/configBound.d.ts",
  "exports": {
    ".": {
      "types": "./dist/configBound.d.ts",
      "require": "./dist/configBound.js",
      "import": "./dist/configBound.js"
    },
    "./createConfig": {
      "types": "./dist/createConfig.d.ts",
      "require": "./dist/createConfig.js",
      "import": "./dist/createConfig.js"
    },
    "./section": {
      "types": "./dist/section/section.d.ts",
      "require": "./dist/section/section.js",
      "import": "./dist/section/section.js"
    },
    "./element": {
      "types": "./dist/element/element.d.ts",
      "require": "./dist/element/element.js",
      "import": "./dist/element/element.js"
    },
    "./bind": {
      "types": "./dist/bind/bind.d.ts",
      "require": "./dist/bind/bind.js",
      "import": "./dist/bind/bind.js"
    },
    "./bind/configValueProvider": {
      "types": "./dist/bind/configValueProvider.d.ts",
      "require": "./dist/bind/configValueProvider.js",
      "import": "./dist/bind/configValueProvider.js"
    },
    "./bind/binds/envVar": {
      "types": "./dist/bind/binds/envVar.d.ts",
      "require": "./dist/bind/binds/envVar.js",
      "import": "./dist/bind/binds/envVar.js"
    },
    "./utilities": {
      "types": "./dist/utilities/index.d.ts",
      "require": "./dist/utilities/index.js",
      "import": "./dist/utilities/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "config",
    "configuration",
    "environment",
    "settings"
  ],
  "author": "Robert Keyser",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@config-bound/eslint-config": "*",
    "@eslint/js": "^9.39.1",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-turbo": "^2.6.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest-runner-groups": "^2.2.0",
    "markdownlint-cli2": "^0.18.1",
    "prettier": "3.6.2",
    "rimraf": "^6.1.0",
    "ts-jest": "^29.4.5",
    "tsc-alias": "^1.8.16",
    "turbo": "2.5.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "dependencies": {
    "joi": "^18.0.1"
  }
}
